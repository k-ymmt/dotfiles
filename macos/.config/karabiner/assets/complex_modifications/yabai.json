{
  "title": "yabai keybindings",
  "rules": [
    {
      "description": "open terminal",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open Terminal.app"
            }
          ]
        }
      ]
    },
    {
      "description": "focus west window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus west"
            }
          ]
        }
      ]
    },
    {
      "description": "focus south window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus south"
            }
          ]
        }
      ]
    },
    {
      "description": "focus north window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus north"
            }
          ]
        }
      ]
    },
    {
      "description": "focus east window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus east"
            }
          ]
        }
      ]
    },
    {
      "description": "swap west window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap west"
            }
          ]
        }
      ]
    },
    {
      "description": "swap south window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap south"
            }
          ]
        }
      ]
    },
    {
      "description": "swap north window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap north"
            }
          ]
        }
      ]
    },
    {
      "description": "swap east window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap east"
            }
          ]
        }
      ]
    },
    {
      "description": "move west window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp west"
            }
          ]
        }
      ]
    },
    {
      "description": "move south window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp south"
            }
          ]
        }
      ]
    },
    {
      "description": "move north window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp north"
            }
          ]
        }
      ]
    },
    {
      "description": "move east window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "fn",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp east"
            }
          ]
        }
      ]
    },
    {
      "description": "balance size of window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --balance"
            }
          ]
        }
      ]
    },
    {
      "description": "make new space",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --create"
            }
          ]
        }
      ]
    },
    {
      "description": "destroy this space",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "fn",
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --destroy"
            }
          ]
        }
      ]
    },
    {
      "description": "send window to next space",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "fn",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --space next; /usr/local/bin/yabai -m space --focus next"
            }
          ]
        }
      ]
    },
    {
      "description": "send window to prev space",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [
                "fn",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --space prev; /usr/local/bin/yabai -m space --focus prev"
            }
          ]
        }
      ]
    },
    {
      "description": "floating / unfloating window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --toggle float; /usr/local/bin/yabai -m window --grid 6:6:1:1:4:4"
            }
          ]
        }
      ]
    },
    {
      "description": "toggle sticky(picture-in-picture size)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --toggle sticky;yabai -m window --grid 5:5:4:4:1:1"
            }
          ]
        }
      ]
    },
    {
      "description": "increase left window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize left:-20:0"
            }
          ]
        }
      ]
    },
    {
      "description": "increase bottom window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize bottom:-20:0"
            }
          ]
        }
      ]
    },
    {
      "description": "increase top window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize top:-20:0"
            }
          ]
        }
      ]
    },
    {
      "description": "increase right window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize right:-20:0"
            }
          ]
        }
      ]
    },
    {
      "description": "decrease left window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "fn",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize left:20:0"
            }
          ]
        }
      ]
    },
    {
      "description": "decrease bottom window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "fn",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize bottom:20:0"
            }
          ]
        }
      ]
    },
    {
      "description": "decrease top window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "fn",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize left:20:0"
            }
          ]
        }
      ]
    },
    {
      "description": "decrease right window size",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "fn",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --resize right:20:0"
            }
          ]
        }
      ]
    }
  ]
}
